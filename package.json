{
  "name": "blog-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run redirects:enable && remix build",
    "dev": "npm run redirects:disable && remix dev",
    "start": "netlify serve",
    "typecheck": "tsc -b",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects"
  },
  "dependencies": {
    "@epic-web/cachified": "^4.0.0",
    "@mdx-js/esbuild": "^3.0.0",
    "@netlify/functions": "^2.0.0",
    "@netlify/remix-adapter": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.8",
    "gray-matter": "^4.0.3",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.294.0",
    "mdx-bundler": "^10.0.1",
    "p-queue": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-mdx-images": "^3.0.0",
    "remix-themes": "^1.2.2",
    "shx": "^0.3.4",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.27.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
